<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.fpdone.reservametaquest.EliminarReservaController">
   <children>
      <TableView fx:id="reservasTableView" layoutX="250.0" layoutY="60.0" onMouseClicked="#seleccionar" prefHeight="600.0" prefWidth="600.0">
         <columns>
            <TableColumn fx:id="instructorColumn" prefWidth="150.0" text="Instructor" />
            <TableColumn fx:id="fechaInicioColumn" prefWidth="150.0" text="Fecha Inicio" />
            <TableColumn fx:id="fechaFinColumn" minWidth="0.0" prefWidth="150.0" text="Fecha Fin" />
            <TableColumn fx:id="cantidadMQColumn" minWidth="0.0" prefWidth="150.0" text="Cantidad de MQ" />
         </columns>
      </TableView>
      <Button fx:id="eliminarReservaBttn" layoutX="925.0" layoutY="323.0" mnemonicParsing="false" onAction="#eliminarReserva" prefHeight="75.0" prefWidth="250.0" text="Eliminar">
         <font>
            <Font size="25.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
